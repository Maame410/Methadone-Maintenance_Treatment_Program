   "source": [
    "#calculate retention rate of patient\n",
    "\"\"\" retention rate = (total days attended by patient / total days) x 100 \"\"\"\n",
    "total_days = 30 #1 month data from program\n",
    "df['retention rate'] = (df['total_days_attended']/ total_days) * 100\n",
    "df"
   ]
  },
